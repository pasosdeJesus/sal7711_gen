<% paisid = Msip.paisomision ?  Msip.paisomision : 170 %>

<%= f.association :departamento,
  collection: Msip::Departamento.habilitados.
  where(pais_id: paisid),
  label_method: :nombre, 
  value_method: :id 
%>
  <%= f.association :municipio,
    collection: Msip::Municipio.habilitados.joins(:departamento).
    where("msip_departamento.pais_id = ?", paisid),
    label_method: :nombre, 
    value_method: :id
  %>
